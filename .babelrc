{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "debug": true,
        "targets": {
          "chrome": 52,
          "firefox": 54,
          "safari": 10,
          "node": 6,
          "edge": 25
        },
        "exclude": [
          "transform-es2015-for-of"
        ]
      }
    ],
    "babili"
  ],
  "plugins": [
    "version-inline"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    }
  }
}
