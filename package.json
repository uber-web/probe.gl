{
  "name": "probe.gl",
  "description": "JavaScript Console Instrumentation and Benchmarking for Browser and Node",
  "license": "MIT",
  "private": "true",
  "keywords": [
    "javascript",
    "profiling",
    "instrumentation",
    "logging"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber-web/probe.gl.git"
  },
  "workspaces": [
    "modules/*"
  ],
  "scripts": {
    "bootstrap": "ocular-bootstrap",
    "clean": "ocular-clean",
    "build": "ocular-clean && ocular-build",
    "lint": "ocular-lint",
    "publish": "ocular-publish",
    "test": "ocular-test",
    "version": "ocular-build core"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^5.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-version-inline": "^1.0.0",
    "coveralls": "^2.13.0",
    "eslint": "^4.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-uber-es2015": "^3.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "lerna": "^2.9.1",
    "module-alias": "^2.0.0",
    "nyc": "^10.2.0",
    "ocular-dev-tools": "0.0.6",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.0",
    "reify": "^0.4.4",
    "source-map-loader": "^0.2.1",
    "tap-browser-color": "^0.1.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.5.1",
    "tape-catch": "^1.0.4",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^2.4.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^2.4.0"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "test/**/*.js",
      "src/layers/deprecated"
    ]
  }
}
