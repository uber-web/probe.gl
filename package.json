{
  "name": "probe.gl-monorepo",
  "description": "JavaScript Console Instrumentation and Benchmarking for Browser and Node",
  "license": "MIT",
  "type": "module",
  "private": true,
  "keywords": [
    "javascript",
    "profiling",
    "instrumentation",
    "logging"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber-web/probe.gl.git"
  },
  "workspaces": [
    "examples/**",
    "modules/*"
  ],
  "browser": {
    "jsdom": false
  },
  "resolutions_comments": {
    "esbuild": "https://github.com/evanw/esbuild/issues/2762"
  },
  "resolutions": {
    "esbuild": "0.16.7"
  },
  "scripts": {
    "bootstrap": "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true yarn && ocular-bootstrap",
    "clean": "ocular-clean",
    "build": "npm run clean && time ocular-build",
    "lint": "tsc && ocular-lint",
    "publish": "ocular-publish",
    "test": "ocular-test",
    "test-fast": "ocular-test fast",
    "version": "ocular-build core"
  },
  "devDependencies": {
    "@types/tape-promise": "^4.0.1",
    "jsdoc-to-markdown": "^3.0.0",
    "jsdom": "^16.5.0",
    "ocular-dev-tools": "2.0.0-alpha.5",
    "pre-commit": "^1.2.2",
    "react-dom": "^16.13.1",
    "url-loader": "^1.0.1"
  },
  "pre-commit": [
    "test-fast"
  ],
  "dependencies": {}
}
