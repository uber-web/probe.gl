{
  "name": "probe.gl",
  "description": "JavaScript Console Instrumentation and Benchmarking for Browser and Node",
  "license": "MIT",
  "private": "true",
  "keywords": [
    "javascript",
    "profiling",
    "instrumentation",
    "logging"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber-web/probe.gl.git"
  },
  "workspaces": [
    "modules/*"
  ],
  "browser": {
    "jsdom": false
  },
  "scripts": {
    "bootstrap": "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true yarn && ocular-bootstrap",
    "clean": "ocular-clean",
    "build": "ocular-clean && ocular-build",
    "lint": "ocular-lint",
    "publish": "ocular-publish",
    "test": "ocular-test",
    "test-fast": "ocular-test fast",
    "version": "ocular-build core"
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "@probe.gl/test-utils": "^3.1.1",
    "babel-loader": "^8.0.0",
    "coveralls": "^2.13.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-uber-es2015": "^3.1.2",
    "eslint-config-uber-jsx": "^3.3.3",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.10",
    "jsdom": "^15.1.1",
    "ocular-dev-tools": "0.0.30",
    "pre-commit": "^1.2.2",
    "reify": "^0.18.1",
    "url-loader": "^1.0.1"
  },
  "pre-commit": [
    "test-fast"
  ]
}
