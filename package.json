    "publish": "ocular-publish",
    "test": "ocular-test",
    "test-fast": "ocular-test fast",
    "version": "ocular-build core"
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.0",
    "coveralls": "^2.13.0",
    "ocular-dev-tools": "0.0.24",
    "pre-commit": "^1.2.2",
    "reify": "^0.18.1",
    "url-loader": "^1.0.1"
  },
  "pre-commit": [
    "test-fast"
